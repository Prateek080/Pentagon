<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
	android:gravity="center"
    android:background="@drawable/lag"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.pentagonfinal.MainActivity" >

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="145dp"
        android:layout_height="145dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40dp"
        android:src="@drawable/pentagon" />

    <ImageButton
        android:id="@+id/recordbtn1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_centerInParent="true"
        android:background="@drawable/imbutton"
        android:src="@drawable/camera_1" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/recordbtn1"
        android:layout_marginRight="21dp"
        android:layout_toLeftOf="@+id/recordbtn1"
        android:background="@drawable/imbutton"
        android:src="@drawable/location" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/recordbtn1"
        android:layout_marginLeft="21dp"
        android:layout_toRightOf="@+id/recordbtn1"
        android:background="@drawable/imbutton"
        android:src="@drawable/alarm" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageButton2"
        android:layout_marginTop="26dp"
        android:layout_toLeftOf="@+id/recordbtn1"
        android:background="@drawable/imbutton"
        android:src="@drawable/call" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_above="@+id/imageButton5"
        android:layout_centerHorizontal="true"
        android:src="@drawable/logo" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageButton4"
        android:layout_toRightOf="@+id/recordbtn1"
        android:background="@drawable/imbutton"
        android:src="@drawable/recordg" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageView1"
        android:layout_alignTop="@+id/imageButton6"
        android:layout_marginLeft="16dp"
        android:textStyle="bold"
        android:layout_toLeftOf="@+id/imageButton5" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView1"
        android:textStyle="bold"
        android:layout_alignRight="@+id/textView1"
        android:layout_alignTop="@+id/imageButton7" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageButton4"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@+id/textView1"
        android:background="@drawable/imbutton"
        android:src="@drawable/cg" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageButton7"
        android:layout_marginTop="5dp"
        android:layout_toLeftOf="@+id/textView1"
        android:background="@drawable/imbutton"
        android:src="@drawable/cg" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageButton6"
        android:layout_marginTop="5dp"
        android:layout_toLeftOf="@+id/textView1"
        android:background="@drawable/imbutton"
        android:src="@drawable/cg" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView2"
        android:textStyle="bold"
        android:layout_alignRight="@+id/textView2"
        android:layout_alignTop="@+id/imageButton8" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageButton8"
        android:layout_below="@+id/imageButton8"
        android:layout_marginTop="5dp"
        android:background="@drawable/imbutton"
        android:src="@drawable/cg" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView3"
        android:textStyle="bold"
        android:layout_alignRight="@+id/textView3"
        android:layout_alignTop="@+id/imageButton9" />

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageButton9"
        android:layout_below="@+id/imageButton9"
        android:layout_marginTop="5dp"
        android:background="@drawable/imbutton"
        android:src="@drawable/cg" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView4"
        android:textStyle="bold"
        android:layout_alignRight="@+id/textView4"
        android:layout_alignTop="@+id/imageButton10" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/imageButton3"
        android:layout_below="@+id/textView2"
        android:background="@drawable/center"
        android:src="@drawable/gol" />

    <ImageButton
        android:id="@+id/imageButton12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/recordbtn1"
        android:layout_toRightOf="@+id/imageButton5"
        android:background="@drawable/imbutton"
        android:src="@drawable/instructions" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageButton11"
        android:layout_alignRight="@+id/imageButton11"
        android:layout_below="@+id/textView4" />

</RelativeLayout>